	journal = {Nature Materials},
	number = {3},
	pages = {311--316},
	title = {Competing correlated states around the zero-field Wigner crystallization transition of electrons in two dimensions},
	url = {https://doi.org/10.1038/s41563-021-01166-1},
	volume = {21},
	year = {2022},
