	doi = {10.1038/17569},
	id = {Bockrath1999},
	isbn = {1476-4687},
	journal = {Nature},
	number = {6720},
	pages = {598--601},
	title = {Luttinger-liquid behaviour in carbon nanotubes},
	url = {https://doi.org/10.1038/17569},
	volume = {397},
	year = {1999},
