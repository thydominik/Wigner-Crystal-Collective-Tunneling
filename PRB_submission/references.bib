	date-modified = {2023-05-28 12:01:19 +0300},
	doi = {10.1038/nphys322},
	id = {Chen2006},
	isbn = {1745-2481},
	journal = {Nature Physics},
	number = {7},
	pages = {452--455},
	title = {Melting of a 2D quantum electron solid in high magnetic field},
	url = {https://doi.org/10.1038/nphys322},
	volume = {2},
	year = {2006},
	bdsk-url-1 = {https://doi.org/10.1038/nphys322}}




